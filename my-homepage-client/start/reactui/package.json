    "http-status-codes": "^2.3.0",
    "immer": "10.1.1",
    "immutable": "4.3.7",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "next": "15.2.4",
    "react": "18.3.1",
    "react-datepicker": "^7.3.0",
    "react-dom": "18.3.1",
